@model IEnumerable<student_attendance.Models.Module>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.course_id_fk.course_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.module_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.module_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.credit_hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.semester)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
