        <td>
            @Html.DisplayFor(modelItem => item.course_id_fk.course_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.module_code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.module_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.credit_hour)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.semester)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.module_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.module_id }) |
